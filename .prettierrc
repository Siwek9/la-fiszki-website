{
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "useTabs": false,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "auto",
    "singleAttributePerLine": true,
    "vueIndentScriptAndStyle": true,
    "plugins": ["prettier-plugin-css-order"]
}
